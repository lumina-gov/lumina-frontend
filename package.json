{
    "name": "frontend",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev --port 8080 --host",
        "prod": "vite preview --port 80 --host",
        "build": "vite build",
        "package": "vite package",
        "preview": "vite preview",
        "check": "svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "eslint --ignore-path .gitignore .",
        "codegen": "(test -f codegen.yml || node codegen.js) && graphql-codegen --config codegen.yml"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "^2.16.4",
        "@graphql-codegen/client-preset": "^1.2.6",
        "@graphql-codegen/introspection": "^2.2.3",
        "@sveltejs/adapter-auto": "^1.0.1",
        "@sveltejs/kit": "^1.1.3",
        "@typescript-eslint/eslint-plugin": "^5.48.2",
        "@typescript-eslint/parser": "^5.48.2",
        "eslint": "^8.32.0",
        "eslint-plugin-svelte": "^2.14.1",
        "svelte": "^3.55.1",
        "svelte-check": "^2.10.3",
        "svelte-preprocess": "^5.0.0",
        "svelte2tsx": "^0.6.0",
        "tslib": "^2.4.1",
        "typescript": "^4.9.4",
        "vite": "^4.0.4"
    },
    "type": "module",
    "dependencies": {
        "@notionhq/client": "^2.2.3",
        "@resvg/resvg-js": "^2.2.0",
        "@urql/core": "^3.1.1",
        "bson": "^4.7.2",
        "marked": "^4.2.12",
        "postcss": "8.4.20",
        "satori": "^0.0.44",
        "schema-dts": "^1.1.0",
        "stylus": "0.55.0",
        "svelte-markdown": "^0.2.3",
        "svelte-material-icons": "^2.0.4"
    }
}
